<h2><%= @mytest %></h2>

<%= @yourresults %>
<div id="nutri-label" style="{border-color: #ff0000;}" >
  <p id="nutri-label-header"><%= @quizquestion.brand_name %></p>
  <p id="nutri-label-info"><%= @quizquestion.item_name %>
    <span id="nutri-label-value"><%= @quizquestion.nf_serving_size_qty %> <%= @quizquestion.nf_serving_size_unit %></p>
  <div id="nutri-bar-thick">&nbsp;</div>
  <p id="nutri-label-strong">Serving Size
  	<span id= "nutri-label-value"><%= @quizquestion.nf_serving_weight_grams %> g</span></p>
  <p id="nutri-label-strong">Calories
  	<span id= "nutri-label-value"><%= @quizquestion.nf_calories %> g</span></p>
  <p id="nutri-label-light">Calories from Fat
    <span id ="nutri-label-value"><%= @quizquestion.nf_calories_from_fat %></span></p>
